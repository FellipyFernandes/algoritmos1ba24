programa {
  funcao inicio() {

    //Declaração
    real v, q, t
    
    //Entrada
    escreva("Digite qual o valor do produto: ", v)
    leia(v)
    escreva("Digite a quantidade: ", q)
    leia(q)
    
    //Processamento
    t = v*q 

    //Saída
    escreva("O valor total a ser pago é: ", t)
  }
}
